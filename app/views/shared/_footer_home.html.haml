#footer
  .container
    .boxes.info
      .box.logo
%head
  %SCRIPT{:LANGUAGE => "JavaScript"}
    /
      hide from none JavaScript Browsers
      Image1= new Image(75,50)
      Image1.src = "footer-logo.png"
      Image2 = new Image(75,50)
      Image2.src = "footer-app-store.png"
      function SwapOut() {
      document.imageflip.src = Image2.src; return true;
      }
      function SwapBack() {
      document.imageflip.src = Image1.src; return true;
      }
      \// - stop hiding

Image1= new Image(75,50)
Image1.src = "footer-logo.png"
Image2 = new Image(75,50)
Image2.src = "footer-app-store.png"


        %img{ :src => image_path("footer-logo.png") }
        %h1
          Love it? Share it!
        %h2
          Qwiqq is a fast, fun, and simple way to share the things you love to buy.

      .box.download
        %a{ :href => "/download" }
          %img{ :src => image_path("footer-app-store.png") }
        %h1
          Free Download
        %h2
          Get Qwiqq for free from the iPhone App Store.

      .box.contact
        .services
          %a{ :href => "http://www.facebook.com/Qwiqq" }
            %img{ :src => image_path("footer-facebook.png") }
          %a{ :href => "http://www.twitter.com/Qwiqq" }
            %img{ :src => image_path("footer-twitter.png") }
          %a{ :href => "mailto:hello@qwiqq.com" }
            %img{ :src => image_path("footer-email.png") }
        %h1
          Get in Touch
        %h2
          Facebook, Tweet or email us, we'd love to hear from you.

%A{:HREF => "www.google.com", :onMouseOut => "SwapBack()", :onMouseOver => "SwapOut()"}
  %IMG{:BORDER => "0", :HEIGHT => "50", :NAME => "imageflip", :SRC => "footer-app-store.png", :WIDTH => "75"}/



